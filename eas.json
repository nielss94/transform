{
  "cli": {
    "version": ">= 12.0.0"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "env": {
        "EXPO_PUBLIC_BUILD_PROFILE": "development",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk",
        "gradleCommand": ":app:assembleRelease"
      },
      "ios": {
        "simulator": true
      },
      "env": {
        "EXPO_PUBLIC_BUILD_PROFILE": "preview",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true"
      },
      "channel": "preview"
    },
    "internal-testing": {
      "distribution": "internal",
      "android": {
        "buildType": "app-bundle",
        "gradleCommand": ":app:bundleRelease"
      },
      "env": {
        "EXPO_PUBLIC_BUILD_PROFILE": "internal-testing",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true"
      },
      "channel": "internal-testing"
    },
    "production": {
      "distribution": "store",
      "ios": {
        "autoIncrement": "buildNumber"
      },
      "android": {
        "autoIncrement": "versionCode",
        "buildType": "app-bundle"
      },
      "env": {
        "EXPO_PUBLIC_BUILD_PROFILE": "production",
        "EXPO_PUBLIC_ENABLE_LOGGING": "false"
      },
      "channel": "production"
    },
    "development-simulator": {
      "developmentClient": true,
      "distribution": "internal",
      "ios": {
        "simulator": true
      },
      "environment": "development",
      "env": {
        "EXPO_PUBLIC_BUILD_PROFILE": "development-simulator",
        "EXPO_PUBLIC_ENABLE_LOGGING": "true"
      },
      "channel": "development"
    }
  },
  "submit": {
    "production": {},
    "internal": {
      "android": {
        "track": "internal"
      }
    },
    "internal-testing": {
      "android": {
        "track": "internal",
        "releaseStatus": "draft"
      }
    }
  }
}